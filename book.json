            "code",

            "splitter",

            "theme-api",

            "github",
            
            "back-to-top-button",
            
            "anchors",

            "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "tbfed-pagefooter": {
          "copyright":"Copyright &yono 2024",
          "modify_label": "Date of revision:",
