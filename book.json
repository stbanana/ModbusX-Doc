        },
        "sidebar-style": {
            "title": "modbusX-Doc",
            "author": "yono"
        },
        "github": {
            "url": "https://github.com/stbanana/modbusX"
        },
        "anchor-navigation-ex": {
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right",
            "multipleH1": false,
            "multipleH2": false,
            "multipleH3": false,
            "multipleH4": false,
            "showLevelIcon": false,
            "showLevel": false
        }
    }
